    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="ResourcesText.fr.Designer.cs">
      <DependentUpon>ResourcesText.fr.resx</DependentUpon>
      <DesignTime>True</DesignTime>
